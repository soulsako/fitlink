{
  "name": "fitlink",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start",
    "start:clear": "cross-env NODE_ENV=development expo start -c",
    "start:dev-client": "cross-env NODE_ENV=development expo start --dev-client",
    "start:dev-client:clear": "cross-env NODE_ENV=development expo start --dev-client -c",
    "web": "cross-env NODE_ENV=development expo start --web",
    "android": "cross-env NODE_ENV=development expo run:android",
    "ios": "cross-env NODE_ENV=development expo run:ios",
    "android:build:dev": "eas build -p android --profile development --clear-cache",
    "android:build:preview": "eas build -p android --profile preview --clear-cache",
    "android:build:prod": "eas build -p android --profile production --clear-cache",
    "ios:build:sim": "eas build -p ios --profile ios-simulator",
    "ios:build:dev-client": "eas build -p ios --profile ios-dev-client",
    "ios:dev-client": "cross-env NODE_ENV=development expo start --dev-client",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "format:check": "biome format . --check",
    "typecheck": "tsc --noEmit",
    "expo:check": "expo config && expo whoami && expo doctor",
    "clean:metro": "bash -lc 'watchman watch-del-all || true; rm -rf $TMPDIR/metro-cache* $TMPDIR/haste-map-* || true'",
    "clean:expo": "rm -rf .expo .expo-shared && expo start -c",
    "clean:cache": "npm run clean:metro && rm -rf node_modules && npm install",
    "cleanup": "rm -rf node_modules package-lock.json .expo .expo-shared && npm install && expo install --fix",
    "build:clean": "rm -rf node_modules package-lock.json .expo && npm install && expo install --check && eas build --platform android --profile development --clear-cache",
    "build:dev": "eas build --platform android --profile development --clear-cache",
    "build:dev-and-start": "eas build --platform android --profile development --clear-cache && echo 'ðŸŽ‰ Build complete! Install the APK and then run: npm run start:dev-client' && echo 'ðŸ“± APK download link will be shown above'",
    "ios:dev:tunnel": "expo start --dev-client --host tunnel --port 8081",
    "ios:dev:lan": "expo start --dev-client --host lan --port 8081"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@hookform/resolvers": "^3.10.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@supabase/supabase-js": "^2.55.0",
    "expo": "53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "^13.3.2",
    "expo-location": "^18.1.6",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-hook-form": "^7.53.2",
    "react-native": "0.79.5",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^2.2.0",
    "@expo/config-types": "^53.0.5",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "expo-build-properties": "^0.14.8",
    "typescript": "5.8.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
