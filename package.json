{
  "name": "fitlink",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start",
    "start:dev-client": "cross-env NODE_ENV=development expo start --dev-client",
    "android": "cross-env NODE_ENV=development expo run:android",
    "ios": "cross-env NODE_ENV=development expo run:ios",
    "web": "cross-env NODE_ENV=development expo start --web",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "expo:check": "npx expo config && npx expo whoami && npx expo doctor",
    "check-format": "biome format . --check",
    "typecheck": "tsc --noEmit",
    "cleanup": "rm -rf node_modules package-lock.json .expo && npx expo install --fix && npm install",
    "dev": "npx expo start --dev-client --clear",
    "build:clean": "rm -rf node_modules package-lock.json .expo && npm install && npx expo install --check && eas build --platform android --profile development --clear-cache",
    "build:dev": "eas build --platform android --profile development --clear-cache",
    "build:dev-and-start": "eas build --platform android --profile development --clear-cache && echo 'ðŸŽ‰ Build complete! Install the APK and then run: npm run dev' && echo 'ðŸ“± APK download link will be shown above'"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@hookform/resolvers": "^3.10.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@supabase/supabase-js": "^2.55.0",
    "expo": "53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-dev-client": "~5.2.4",
    "expo-font": "^13.3.2",
    "expo-location": "^18.1.6",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-hook-form": "^7.53.2",
    "react-native": "0.79.5",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "zod": "^3.23.8",
    "expo-crypto": "~14.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^2.2.0",
    "@expo/config-types": "^53.0.5",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "typescript": "5.8.3"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true
}
