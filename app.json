      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/app/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
