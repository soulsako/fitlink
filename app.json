  "expo": {
    "name": "fitlink",
    "slug": "fitlink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "fitlink",
    "splash": {
      "image": "./assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
